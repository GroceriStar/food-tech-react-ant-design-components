{
  "name": "@groceristar/cards-wrapper",
  "version": "1.0.21",
  "description": "module to use antd cards between projects",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": "https://github.com/GroceriStar/cards-wrapper-component",
  "author": "Arthur Tkachenko",
  "license": "MIT",
  "private": false,
  "scripts": {
    "bundle": "rollup -c",
    "build": "babel src -d dist",
    "build:test": "shx rm -rf && babel src -d dist",
    "#prepare": "yarn clean",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "lint": "standard",
    "zprepublish": "yarn clean && yarn build",
    "test": "CI=true && jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "code-fix": "standard --fix",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "babel-plugin-module-resolver": "4.0.0",
    "coveralls": "3.0.9",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "jest": "27.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "regenerator-runtime": "0.13.3",
    "rollup": "2.0.3",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "shx": "0.3.2",
    "standard": "14.3.3"
  },
  "dependencies": {
    "@react-pdf/renderer": "^1.5.1",
    "antd": "^4.0.0",
    "react-image": "^2.1.1",
    "uuid": "^8.0.0",
    "snyk": "^1.192.0"
  },
  "peerDependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "snyk": true
}
