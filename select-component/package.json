    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "25.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "jest": "25.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
